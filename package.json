{
  "name": "focus-no-jutsu",
  "version": "1.0.0",
  "type": "module",
  "description": "焦点管理。Focus management.",
  "main": "output/focus-no-jutsu.cjs",
  "exports": {
    "import": "./output/focus-no-jutsu.esm.js",
    "require": "./output/focus-no-jutsu.cjs"
  },
  "sideEffects": false,
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "index.js",
    "index.d.ts",
    "utils.js",
    "output",
    "images"
  ],
  "types": "index.d.ts",
  "scripts": {
    "start": "npm run compile:demo -- --watch",
    "build": "npm run compile",
    "compile": "npm run compile:esm && npm run compile:cjs && npm run compile:umd",
    "compile:demo": "BUILD_ENV=demo BABEL_ENV=es5 rollup -c",
    "compile:esm": "BUILD_ENV=esm BABEL_ENV=esm rollup -c",
    "compile:cjs": "BUILD_ENV=cjs BABEL_ENV=es5 rollup -c",
    "compile:umd": "BUILD_ENV=umd BABEL_ENV=es5 rollup -c",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wswmsword/focus-no-jutsu.git"
  },
  "keywords": [
    "focus",
    "focus-management",
    "accessibility",
    "keyboard",
    "tab-sequence",
    "focus-lock",
    "focus-trap"
  ],
  "author": "wsWmsword",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wswmsword/focus-no-jutsu/issues"
  },
  "homepage": "https://github.com/wswmsword/focus-no-jutsu#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rollup": "^3.20.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2"
  }
}
